@{
    ViewBag.Title = "Stock Portfolio";
}

@section scripts {
    <link href="~/Content/themes/fullcalendar/fullcalendar.print.css" rel="stylesheet" />
    <link href="~/Content/themes/fullcalendar/fullcalendar.css" rel="stylesheet" />
    <script src="~/Scripts/moment.min.js"></script>
    <script src="~/Scripts/jquery.min.js"></script>
    <script src="~/Scripts/jquery-ui.custom.min.js"></script>
    <script src="~/Scripts/fullcalendar.min.js"></script>
    <script>


        $(document).ready(function () {
            events();
        });

    </script>
    <style>
        body {
            margin: 0;
            padding: 0;
            font-family: "Lucida Grande",Helvetica,Arial,Verdana,sans-serif;
            font-size: 14px;
        }

        #script-warning {
            display: none;
            background: #eee;
            border-bottom: 1px solid #ddd;
            padding: 0 10px;
            line-height: 40px;
            text-align: center;
            font-weight: bold;
            font-size: 12px;
            color: red;
        }

        #loading {
            display: none;
            position: absolute;
            top: 10px;
            right: 10px;
        }

        #calendar {
            width: 900px;
            margin: 40px auto;
        }
    </style>
}

@section featured {

    <div class="jumbotron">
        <div class="container">
            <h1>@ViewBag.Title - Coming Soon!</h1>

        </div>
    </div>

    <div id='loading'></div>
    <div id='calendar'></div>

    <div id="addPhoto" style="float:left;">
        <button onclick="events()"> Add Photo</button>
    </div>
}
