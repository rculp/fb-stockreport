@{
    ViewBag.Title = "Stock Portfolio";
}

@section scripts {
    <link href="~/Content/themes/fullcalendar/fullcalendar.print.css" rel="stylesheet" />
    <link href="~/Content/themes/fullcalendar/fullcalendar.css" rel="stylesheet" />
    <script src="~/Scripts/moment.min.js"></script>
    <script src="~/Scripts/jquery.min.js"></script>
    <script src="~/Scripts/jquery-ui.custom.min.js"></script>
    <script src="~/Scripts/fullcalendar.min.js"></script>

    <style>
        #calendar {
            width: 900px;
            margin: 40px auto;
        }
    </style>
}

@section featured {

    <div class="jumbotron">
        <div class="container">
            <h1>@ViewBag.Title - Coming Soon!</h1>

        </div>
    </div>

    <div id='calendar'></div>

    
}
